# PrintMaster Agent SELinux File Contexts
# Maps filesystem paths to SELinux security contexts

# Executable
/usr/bin/printmaster-agent    --    gen_context(system_u:object_r:printmaster_exec_t,s0)

# Config directory and files
/etc/printmaster(/.*)?              gen_context(system_u:object_r:printmaster_etc_t,s0)

# Data directory (SQLite DB, agent_id, etc.)
/var/lib/printmaster(/.*)?          gen_context(system_u:object_r:printmaster_var_lib_t,s0)

# Log directory
/var/log/printmaster(/.*)?          gen_context(system_u:object_r:printmaster_log_t,s0)
