# PrintMaster Agent Configuration
# This file allows deployment-specific overrides without modifying the database.
# Rename to config.ini to use. All settings are optional.

# Asset ID extraction regex (default: \b\d{5}\b for 5-digit numeric tags)
#asset_id_regex=\b\d{5}\b

# SNMP community string (default: public)
#snmp_community=public

# Enable debug logging (true/false, default: false)
#debug_logging=false

# Enable parse debug dumps (true/false, default: false)
#dump_parse_debug=false

# SNMP timeout in milliseconds (default: 2000)
#snmp_timeout_ms=2000

# SNMP retries (default: 1)
#snmp_retries=1

# Discovery concurrency (number of parallel workers, default: 50)
#discover_concurrency=50

# ============================================================
# Server Integration (for multi-agent deployments)
# ============================================================

# Enable server communication (true/false, default: false)
#server_enabled=false

# Server URL (HTTPS recommended, required if server_enabled=true)
# Examples:
#   https://printmaster.company.com        (production with Let's Encrypt)
#   https://printmaster.company.com:9443   (custom HTTPS port)
#   http://localhost:9090                  (development only - not secure!)
#server_url=https://printmaster.company.com

# Unique agent identifier (required if server_enabled=true)
# Use descriptive names like: agent-headquarters, agent-warehouse-1
#agent_id=agent-001

# Server CA certificate path (optional)
# Required for self-signed server certificates in development
# Leave empty for Let's Encrypt or other public CAs (uses system trust store)
# Example: Place server's server.crt file here and specify path
#server_ca_path=server-ca.pem

# How often to upload devices to server (seconds, default: 300 = 5 minutes)
#server_upload_interval=300

# How often to send heartbeat to server (seconds, default: 60 = 1 minute)
#server_heartbeat_interval=60

